function carWash(arr) {
    let value = 0
    arr.forEach((command) => {
        switch (command) {
            case "soap":
                value += 10
                break;
            case "water":
                value += value * 0.2
                break;
            case "vacuum cleaner":
                value += value * 0.25
                break;
            case "mud":
                // decrease the value by 10%
                value -= value * 0.1
                break;
        }
    })
    return `The car is ${(value).toFixed(2)}% clean.`
}
console.log(carWash(["soap", "water", "mud", "mud", "water", "mud", "vacuum cleaner"]
))
